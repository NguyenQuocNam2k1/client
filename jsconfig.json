{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "~a/*": [
        "asset/*"
      ],
      "~h/*": [
        "hooks/*"
      ],
      "~r/*": [
        "src/redux/*"
      ],
      "-cc/*": [
        "src/components/core/*"
      ],
      "-cl/*": [
        "src/components/layout/*"
      ],
      "-cp/*": [
        "src/components/pages/*"
      ],
      "-cr/*": [
        "src/components/routes/*"
      ]
    }
  }
}